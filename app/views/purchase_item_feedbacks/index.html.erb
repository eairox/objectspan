<h1>Listing purchase_item_feedbacks</h1>

<table>
  <tr>
    <th>Purchase item</th>
    <th>Item rating</th>
    <th>Buy again in next 6 month</th>
    <th>Recommends to friend</th>
    <th>Feedback improvement</th>
    <th>Item feedback rating question</th>
    <th>Item feedback answer comments</th>
    <th>Item feedback answer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @purchase_item_feedbacks.each do |purchase_item_feedback| %>
  <tr>
    <td><%= purchase_item_feedback.purchase_item_id %></td>
    <td><%= purchase_item_feedback.item_rating %></td>
    <td><%= purchase_item_feedback.buy_again_in_next_6_month %></td>
    <td><%= purchase_item_feedback.recommends_to_friend %></td>
    <td><%= purchase_item_feedback.feedback_improvement %></td>
    <td><%= purchase_item_feedback.item_feedback_rating_question_id %></td>
    <td><%= purchase_item_feedback.item_feedback_answer_comments %></td>
    <td><%= purchase_item_feedback.item_feedback_answer %></td>
    <td><%= link_to 'Show', purchase_item_feedback %></td>
    <td><%= link_to 'Edit', edit_purchase_item_feedback_path(purchase_item_feedback) %></td>
    <td><%= link_to 'Destroy', purchase_item_feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

