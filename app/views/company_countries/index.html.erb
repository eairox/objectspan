<h1>Listing company_countries</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Country</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @company_countries.each do |company_country| %>
  <tr>
    <td><%= company_country.company_id %></td>
    <td><%= company_country.country_id %></td>
    <td><%= link_to 'Show', company_country %></td>
    <td><%= link_to 'Edit', edit_company_country_path(company_country) %></td>
    <td><%= link_to 'Destroy', company_country, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

